<?xml version="1.0"?>
<odoo>
    <record id="order_list_view_tree" model="ir.ui.view">
        <field name="name">order.list.tree</field>
        <field name="model">order.list</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="item_name"/>
                <field name="description"/>
                <field name="quantity"/>
                <field name="unit_price"/>
                <field name="sub_total_price"/>
            </tree>


        </field>
    </record>
</odoo>